# Telegram API Credentials (get from https://my.telegram.org)
TELEGRAM_APP_ID=your_app_id
TELEGRAM_API_HASH=your_api_hash

# Server Config
PORT=3001
NODE_ENV=development

# Session (auto-generated after login)
SESSION_STRING=

# TTS Providers (optional - cho các giọng nói premium)
# OpenAI TTS - https://platform.openai.com/api-keys
OPENAI_API_KEY=

# Google Cloud TTS - https://console.cloud.google.com/apis/credentials
# Cần enable "Cloud Text-to-Speech API" trong GCP Console
GOOGLE_CLOUD_API_KEY=
